.form-multiselectfield {
  .multiselect {
    @include font.font-normal();
    margin-top: 6px;
    .multiselect__tags {
      max-height: 40px;
      padding: 0px 32px 0 2px;
      border-radius: 0;
      border: none;
      border-bottom: 1px solid globals.$color-gray35;
      overflow: hidden;
      background-color: transparent;
      span {
        font-size: .8rem;
      }
      .multiselect__placeholder {
        color: globals.$color-gray60;
        font-size: 1rem;
        padding-top: 15px;
      }
      .multiselect__strong {
        line-height: 40px;
        font-weight: normal;
        color: globals.$color-gray25;
      }
      .multiselect__single {
        padding: 5px 30px 0 3px;
        margin-bottom: 0;
        @include font.font-normal();
        font-size: 1rem;
        color: globals.$color-gray60;
        height: 40px;
        line-height: 40px;
        background: transparent;
      }
      .multiselect__tags-wrap {
        visibility: hidden;
        display: inline-block;
        height: 24px;
        overflow: hidden;
        margin-top: 15px;
        .multiselect__tag {
          border-radius: 2px;
          margin-right: 5px;
          background: globals.$color-gray25;
          .multiselect__tag-icon {
            &:after {
              color: globals.$color-gray50;
            }
            &:hover {
              background: globals.$color-gray15;
              &:after {
                color: globals.$color-gray80;
              }
            }
          }
        }
      }

    }
    .multiselect__select {
      width: 30px;
      &:before {
        border-width: 6px 6px 0 6px;
        top: 85%;
      }
    }
  }
  .multiselect--active .multiselect__select {
    transform: rotateX(0deg);
  }
  .multiselect__content-wrapper {
    border-color: globals.$color-gray50;
    border-radius: 0;
    .multiselect__content {
      .multiselect__element {

        .multiselect__option {
          @include font.font-normal();
          font-size: 1rem;
          color: globals.$color-gray60;
          min-height: 20px;
          padding: 6px 30px 6px 12px;
        }
        .multiselect__option:after {
          top: auto;
          right: 0;
          position: absolute;
          line-height: 1rem;
          padding-right: 6px;
          background: transparent;
          content: '';
        }
        .multiselect__option--highlight {
          background-color: globals.$color-gray2;
          &:after {
            background-color: globals.$color-gray2;
          }
        }
        .multiselect__option--selected {
          background-color: globals.$color-gray2;
          &:after {
            content: '\F12C';
            float: left;
            //@include font.font-icon();
            font-size: 1rem;
            color: globals.$color-secondary;
            background-color: transparent;
          }
          &:hover:after {
            content: '\F156';
            color: globals.$color-unavailable;
            background-color: transparent;
          }
        }
      }
    }
  }
}

.form-selectfield {
