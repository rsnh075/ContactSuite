.form-textfield {
  position: relative;
  width: 100%;
  padding-bottom: 13px;
  input[type='text'],
  input[type='email'],
  input[type='password'],
  input[type='number'],
  input[type='tel'] {
    @include font.font-normal();
    height: 40px;
    line-height: 40px;
    width: 100%;
    border: none;
    border-bottom: 1px solid globals.$color-gray35;
    background-color: transparent;
    padding-left: 3px;
    padding-top: 10px;
    resize:none;
    color: globals.$color-gray60;
    font-size: 1rem;
    margin-top: 12px;
    &:focus {
      background-color: transparent;
      & + label {
        top: 8px;
        opacity: 1;
        height: auto;
      }
    }
    &:disabled {
      border-bottom: 1px solid globals.$color-gray25;
      color: globals.$color-gray35;
      user-select: none;
    }
    &.smaller-font {
      font-size: 0.9rem;
      color: globals.$color-gray60;
    }
  }
  &--search,
  &--date {
    padding: 0 10px;
    input[type='text'] {
      @include font.font-normal();
      margin: 0;
      border-bottom: 1px solid tint(globals.$color-white, 80%);
      text-indent: 24px;
      color: globals.$color-white;
      &:placeholder-shown {
        opacity: .5;
      }
    }
    &:before {
      content: '\F349';
      position: absolute;
      top: 65%;
      transform: translateY(-50%);
      @include font.font-icon();
      font-size: 1.3rem;
      display: inline-block;
      color: globals.$color-white;
    }
    &-on-white {
      margin-bottom: 1.4rem;
      padding: 0px;
      input[type='text'] {
        border-bottom: 1px solid globals.$color-gray30;
        color: globals.$color-gray60;
      }
      &:before {
        color: globals.$color-gray30;
      }
    }
    &-on-gray-small {
      max-width: 300px;
      float: left;
      margin-top: .6rem;
      input[type='text'] {
        border-bottom: 1px solid globals.$color-gray30;
        color: globals.$color-gray60;
        &:placeholder-shown {
          opacity: .8;
        }
      }
      &:before {
        color: globals.$color-gray30;
      }
    }
  }
  &--date {
    input[type='text'] {
      text-indent: 2px;
    }
    &:before {
      content: '\FDFA';
      right: 15px;
      pointer-events:none;
      user-select: none;
    }
  }
  &--domain-add {
    input[type='text'],
    input[type='email'],
    input[type='password'],
    input[type='number'],
    input[type='tel'] {
      width: 60%;
      float: left;
      @include font.font-normal();
    }
    span {
      width: calc(40% - 10px);
      padding: 0;
      float: left;
      font-size: 1rem;
      color: globals.$color-gray35;
      @include font.font-normal();
      position: absolute;
      bottom: 10px;
      right: 10px;
    }
  }
  &--bground {
    margin-top: 5px;
    label {
      top: 2px;
      opacity: 1;
      height: auto;
    }
    input[type='text'],
    input[type='email'],
    input[type='password'],
    input[type='number'],
    input[type='tel'] {
      box-shadow: inset 0 -30px 0px 0px rgba(0,0,0,.025);
      &:focus + label {
        top: 2px;
      }
      &:read-only {
        box-shadow: none;
        opacity: 1;
        border-bottom: none;
        color: globals.$color-black;
      }
    }
  }
  &--mask-readonly {
    input[type='text'],
    input[type='email'],
    input[type='password'],
    input[type='number'],
    input[type='tel'] {
      &:read-only {
        box-shadow: inset 0 -30px 0px 0px rgba(0,0,0,.025);
        border-bottom: 1px solid globals.$color-gray35;
        opacity: 1;
        color: globals.$color-gray60;
      }
    }
  }
  &--valid {
    //@include add-checkmark(3, 0, globals.$color-green);
    input[type='text'],
    input[type='email'],
    input[type='password'],
    input[type='number'],
    input[type='tel'] {
      border-bottom: 2px solid globals.$color-green;
    }
  }
  &--small__col--10 {
    margin-left: 10px;
    padding-right: 6px;
    float: left;
    width: calc(10% - 6px);
    min-width: calc(10% - 6px);
    input[type="text"],
    input[type="number"] {
      float: left;
      margin: 0;
      padding: 4px 0 0 0;
      height: 32px;
      text-align: center;
      font-size: 0.8rem;
      border: 0;
      border-bottom: 1px solid globals.$color-gray35;
      overflow: hidden;
      white-space: nowrap;
      text-overflow: ellipsis;
    }
  }
  &--inline-btn-right {
    float: left;
    width: calc(100% - 50px);
  }
}

