.form-button,
.form-button__secondary {
  position: relative;
  margin-top: 1.2rem;
  input[type='button'],
  input[type='submit'],
  button {
    position: relative;
    @include font.font-normal();
    font-size: 1.2rem;
    width: auto;
    height: 40px;
    line-height: 1;
    border: none;
    cursor: pointer;
    appearance: none;
  }
  &.form-button__secondary--lowered {
    margin-top: 0;
    padding-bottom: 0;
  }
  &--disabled button {
    pointer-events: none;
    background-color: globals.$color-gray15;
    box-shadow: none;
    &:hover {
      cursor: default;
      background-color: globals.$color-gray15;
    }
  }
}

.form-button {
  input[type='button'],
  input[type='submit'],
  button {
    padding: 5px 30px;
    color: globals.$color-white;
    padding: 5px 30px;
    background-color: globals.$color-primary;
    border-radius: 3px;
    box-shadow: 0 2px 4px 0 rgba(0,0,0,.2);
  }
